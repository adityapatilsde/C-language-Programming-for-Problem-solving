#include <stdio.h>

int main() {
    int grade = 2;

    printf("Grade level %d:\n", grade);

    switch (grade) {
        case 1:
            printf("Beginner\n");
        case 2:
            printf("Intermediate\n");
        case 3:
            printf("Advanced\n");
        default:
            printf("Undefined Level\n");
    }

    return 0;
}