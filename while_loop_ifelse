#include <stdio.h>

int main() {
    int num;

    // Take user input
    scanf("%d",&num);
    
    while (num != 0) {  // Loop until user enters 0
        if (num % 2 == 0) {
            printf("%d is EVEN.\n", num);
        } 
        else {
            printf("%d is ODD.\n", num);
        }

        scanf("%d", &num);  
    }
    printf("program ended.\n");
    
    return 0;
}